{
    "name": "riconijeboer/laravel-to-swagger",
    "description": "This package aims to export your endpoints to Swagger YAML or JSON files.",
    "keywords": [
        "riconijeboer",
        "laravel",
        "laravel-to-swagger"
    ],
    "homepage": "https://github.com/riconijeboer/laravel-to-swagger",
    "license": "MIT",
    "authors": [
        {
            "name": "Rico Nijeboer",
            "email": "rico@riconijeboer.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "cerbero/lazy-json": "^1.1",
        "fakerphp/faker": "^1.14",
        "illuminate/contracts": "^8.37",
        "illuminate/database": "^8.37",
        "soapbox/laravel-formatter": "2.x",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "RicoNijeboer\\Swagger\\": "src",
            "RicoNijeboer\\Swagger\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicoNijeboer\\Swagger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RicoNijeboer\\Swagger\\SwaggerServiceProvider",
                "RicoNijeboer\\Swagger\\Providers\\ValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
